<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".Dash">


    <TextView
        android:id="@+id/textView"
        android:layout_width="196dp"
        android:layout_height="62dp"
        android:layout_marginBottom="72dp"
        android:fontFamily="@font/roboto_bold"
        android:text="@string/app_name"
        android:textColor="#000000"

        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.182"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/addStudentButton"
        android:layout_width="166dp"
        android:layout_height="70dp"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="96dp"
        android:background="@drawable/squ_tile"
        android:drawableLeft="@drawable/baseline_person_add_alt_1_24"
        android:text="Add Student"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="166dp"
        android:layout_height="70dp"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/squ_tile"
        android:drawableStart="@drawable/baseline_class_24"
        android:text="Create Class"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/addStudentButton"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="166dp"
        android:layout_height="70dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/squ_tile"
        android:drawableStart="@drawable/baseline_payments_24"
        android:text="Classes"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="166dp"
        android:layout_height="70dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="96dp"

        android:background="@drawable/squ_tile"
        android:drawableLeft="@drawable/baseline_person_24"
        android:text="Students"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="320dp"
        android:layout_height="96dp"
        android:layout_marginBottom="104dp"
        android:background="@drawable/long_tile"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <View
        android:id="@+id/view5"
        android:layout_width="320dp"
        android:layout_height="90dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/long_tile"
        app:layout_constraintBottom_toTopOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="241dp"
        android:layout_height="62dp"
        android:layout_marginBottom="48dp"
        android:text="Students"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.756"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/studentCount"
        android:layout_width="58dp"
        android:layout_height="59dp"
        android:layout_marginBottom="56dp"
        android:text="04"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/classCount"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.179"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/classCount"
        android:layout_width="58dp"
        android:layout_height="59dp"
        android:layout_marginStart="60dp"
        android:layout_marginBottom="116dp"
        android:text="02"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="241dp"
        android:layout_height="62dp"
        android:layout_marginBottom="112dp"
        android:text="Classes"
        android:textColor="@color/black"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.776"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>